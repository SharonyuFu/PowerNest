<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codepath.powernest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeEventActivity" />
        <activity android:name=".EventDetailActivity" />
        <activity android:name=".SearchOnMapActivity" />
        <activity android:name=".PowerRushActivity" />


        <activity android:name=".PowerGroupActivity" />
        <activity android:name=".GropuDetailActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".LikedGroupActivity"></activity>

    </application>

</manifest>